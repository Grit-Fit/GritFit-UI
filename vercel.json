{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.gritfit.site/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
        
      ]
    }
  ]
}
